<svg class="ember-divider-dots__inner-dot-container" viewBox={{viewBox}}>
    {{#each (range 1 numDots) as |dot|}}
      {{component dotComponent class="ember-divider-dots__dot" containerComponent=this}}
    {{/each}}
  {{!--{{#if hasBlock}}

    {{yield}}

  {{else}}

    {{#each (range 1 numDots) as |dot|}}
      {{component dotComponent class="ember-divider-dots__dot" containerComponent=this}}
    {{/each}}

  {{/if}}--}}
</svg>