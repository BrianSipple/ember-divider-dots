<svg class="ember-divider-dots__inner-dot-container" viewBox="0 0 100 100">
  {{#if hasBlock}}

    {{yield dots}}

  {{else}}

    {{#each dotCoords as |coords|}}
      {{component dotComponent
        class="ember-divider-dots__dot"
        coords=coords
        size=dotSize
        radius=dotRadius
      }}
    {{/each}}

  {{/if}}
</svg>