<svg class="ember-divider-dots-container__inner" viewBox={{viewBox}}>
  {{#if hasBlock}}

    {{yield}}

  {{else}}

    {{#each dotRange as |dot|}}
      {{component
        dotComponent
        class="ember-divider-dots-container__dot"
        containerComponent=this
      }}
    {{/each}}

  {{/if}}
</svg>