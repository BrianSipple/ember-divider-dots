  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Dummy App",
      "program": "${workspaceRoot}/node_modules/.bin/ember",
      "args": [
        "serve"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Tests",
      "program": "${workspaceRoot}/node_modules/.bin/ember",
      "args": [
        "test"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Tests | Serve",
      "program": "${workspaceRoot}/node_modules/.bin/ember",
      "args": [
        "test",
        "--serve"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Process",
      "address": "localhost",
      "port": 5858
    }
  ]
}
